{
    "output": "serferals.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "extract": false,
    "files": [
        "LICENSE.md",
        "app/config/containers.yml",
        "app/config/media.json",
        "app/config/language.json"
    ],
    "finder": [
       {
            "name": ["*.php"],
            "exclude": ["Tests", "tests", "vendor"],
            "in": ["vendor"]
       },
       {
            "name": ["*.php"],
            "exclude": ["Tests", "tests"],
            "in": ["src"]
       },
       {
            "name": ["*.yml"],
            "in": ["app"]
       }
    ],
    "stub": "src/Resources/phar-stub.php",
    "web": false,
    "git-commit": "git-commit"
}
