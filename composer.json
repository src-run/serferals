{
    "name"        : "rmf/serferals-media-organizer",
    "license"     : "MIT",
    "type"        : "project",
    "homepage"    : "https://get.src.run/serferals-media-organizer",
    "description" : "A CLI application for resolving and organizing movie and episode files.",
    "support" : {
        "email"  : "rmf@src.run",
        "issues" : "https://get.src.run/serferals-media-organizer/issues",
        "source" : "https://get.src.run/serferals-media-organizer/git",
        "irc"    : "irc://irc.efnet.org/src-run"
    },
    "authors" : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@src.run",
            "homepage" : "https://src.run/rmf",
            "role"     : "Project Lead"
        }
    ],
    "keywords" : [
        "serferals-media-organizer", "serferals", "media-organizer", "console", "application", "rmf", "src-run"
    ],
    "require" : {
        "symfony/console"              : "^3.0",
        "symfony/finder"               : "^3.0",
        "symfony/yaml"                 : "^3.0",
        "symfony/serializer"           : "^3.0",
        "symfony/dependency-injection" : "^3.0",
        "symfony/config"               : "^3.0",
        "mhor/php-mediainfo"           : "^1.6",
        "twig/twig"                    : "^1.21,<2.0",
        "php-tmdb/api"                 : "^2.0",
        "monolog/monolog"              : "^1.18",
        "src-run/augustus-library"     : "^0.5",
        "src-run/wonka-library"        : "^0.5"
    },
    "require-dev" : {
        "symfony/var-dumper"     : "^3.0",
        "phpunit/phpunit"        : "^5.0",
        "satooshi/php-coveralls" : "^1.0"
    },
    "autoload" : {
        "psr-4" : {
            "RMF\\Serferals\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "RMF\\Serferals\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config" : {
        "bin-dir" : "bin"
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "2.0-dev"
        }
    }
}
